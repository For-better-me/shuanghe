/*special item*/
.special{
    position: relative;
}
.special .bd{
    width: 1100px;
    height:auto;
    margin:0 auto;
}
.special .bd ul{
    font-size: 0;
}
.special .bd ul>li{
    display: inline-block;
    width: 83px;
    border-right: #dcdcdc solid 1px;
    border-bottom: #dcdcdc solid 1px;
    position: relative;
   
}
.special .bd ul>li:after{
    content: "";
    display: block;
    width: 50%;
    height: 50%;
    background: #1e84aa;
    opacity: 0;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: -1;
    margin:auto;
     transition: all 0.4s linear;
    -moz-transition: all 0.4s linear; /* Firefox 4 */
    -webkit-transition: all 0.4s linear; /* Safari 和 Chrome */
    -o-transition: all 0.4s linear; 
}
.special .bd ul>li:hover:after{
    opacity: 1;
    width: 100%;
    height: 100%;
}
.special .bd ul>li:hover span,.special .bd ul>li:hover a{
    color: #fff;
}
.special .bd ul>li:last-of-type{
    border-right: none;
}
.special .bd ul>li>a{
    display: block;
    text-align: center;
    line-height: 36px;
    font-size: 12px;
    color: #333;
    height: 100%;
}
.special .bd ul>li>a>span{
    text-align: center;
    display: block;
    padding-top: 16px;
}
.special .bd ul>li>a .iconfont{}

